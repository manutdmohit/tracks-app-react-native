{
  "expo": {
    "name": "Tracks",
    "slug": "tracks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An app built to track users' location. For example, This app can be used for travelling as it records the locations of the user from a starting point to an endpoint as per the user's request.  The record can be saved and stored in the database. ",
    "githubUrl": "https://github.com/manutdmohit/tracks-app-react-native"
  }
}
